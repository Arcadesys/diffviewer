{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "types": ["node", "obsidian"]
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
